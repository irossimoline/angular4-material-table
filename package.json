{
  "name": "@e-is/ngx-material-table",
  "description": "Angular 17 table based on @angular/cdk table structure, to allow row insertion, edition, validation and deletion.",
  "version": "17.0.0",
  "license": "MIT",
  "author": "Ignacio Rossi <irossimoline>",
  "keywords": [
    "angular",
    "angular 17",
    "table",
    "inline validation",
    "material design",
    "angular material table",
    "ngx material table",
    "ngx table"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/e-is/ngx-material-table.git"
  },
  "homepage": "https://github.com/e-is/ngx-material-table",
  "bugs": {
    "url": "https://github.com/e-is/ngx-material-table/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development --port 4201",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "packagr": "ng-packagr -p ng-package.json",
    "publish": "npm run packagr && cd dist && npm publish",
    "publish-dev": "npm run packagr && cd dist && npm publish --userconfig=.local/.npmrc",
    "prepare": "npx husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,html}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{css,json,md,scss}": "prettier --write"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "bundles/ngx-material-table.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "private": false,
  "dependencies": {
    "clone": "^2.1.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1702.3",
    "@angular-devkit/build-angular": "~17.2.3",
    "@angular-devkit/core": "~17.2.3",
    "@angular-devkit/schematics": "~17.2.3",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular-eslint/utils": "17.2.1",
    "@angular/animations": "~17.2.4",
    "@angular/cdk": "~17.2.2",
    "@angular/cli": "~17.2.3",
    "@angular/common": "~17.2.4",
    "@angular/compiler": "~17.2.4",
    "@angular/compiler-cli": "~17.2.4",
    "@angular/core": "~17.2.4",
    "@angular/forms": "~17.2.4",
    "@angular/language-service": "~17.2.4",
    "@angular/material": "~17.2.2",
    "@angular/platform-browser": "~17.2.4",
    "@angular/platform-browser-dynamic": "~17.2.4",
    "@angular/router": "~17.2.4",
    "@rollup/plugin-commonjs": "~17.1.0",
    "@rollup/plugin-node-resolve": "~15.2.3",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "~2.0.13",
    "@types/node": "~18.18.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-prettier": "latest",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "material-design-icons": "^3.0.1",
    "ng-packagr": "~17.2.1",
    "ng-table-virtual-scroll": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "husky": "^8.0.3",
    "prettier": "^3.2.5",
    "rollup": "^2.34.0",
    "rxjs": "^7.8.1",
    "ts-node": "~10.9.2",
    "tslib": "~2.3.0",
    "typescript": "~5.3.3"
  },
  "peerDependencies": {
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">= 18.18.2",
    "npm": ">= 9.9.1",
    "yarn": ">= 1.22.0"
  }
}
